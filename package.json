{
  "name": "seedream-mcp-server",
  "version": "1.3.0",
  "description": "MCP server for SeeDream 4.5 image generation - enables Claude Code to generate images via natural language",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "seedream-mcp": "dist/index.js"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "start": "node dist/index.js",
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run build",
    "postinstall": "npm run build"
  },
  "engines": {
    "node": ">=20"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "claude",
    "claude-code",
    "image-generation",
    "ai",
    "seedream",
    "byteplus",
    "text-to-image",
    "image-editing"
  ],
  "author": "SeeDream Gallery",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/syh52/seedream-mcp-server"
  },
  "homepage": "https://github.com/syh52/seedream-mcp-server#readme",
  "bugs": {
    "url": "https://github.com/syh52/seedream-mcp-server/issues"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.6.1",
    "@types/express": "^4.17.21",
    "axios": "^1.7.9",
    "express": "^4.21.2",
    "firebase-admin": "^13.6.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^22.10.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  }
}
